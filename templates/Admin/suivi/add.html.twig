{% extends "Admin/base.html.twig" %}
{% block body %}
<form  method="post">
    {{form_errors(form1)}}
    
    {{ form_start(form1, {'attr': {'novalidate': 'novalidate'}}) }}
    <div class="input-form mt-3">
        {{ form_label(form1.IdLivraison , 'ID Delivery') }}
        {{ form_widget(form1.IdLivraison, {'attr': {'class': 'form-control' }}) }}
    
    </div>
    
    <div class="input-form mt-3">
        {{ form_label(form1.dateComm , ' Estimated Order Date') }}
        {{ form_widget(form1.dateComm, {'attr': {'class': 'form-control' }}) }}
        {{ form_errors(form1.dateComm) }} 
    </div>
    <div class="input-form">
        {{ form_label(form1.localisatione , 'location') }}
        {{ form_widget(form1.localisatione, {'attr': {'class': 'form-control' }}) }}
        {{ form_errors(form1.localisatione) }} 

    </div>
    <div class="input-form">
        {{ form_label(form1.IDComm , 'Order ID') }}
        {{ form_widget(form1.IDComm, {'attr': {'class': 'form-control' }}) }}

    </div>
    <div class="input-form">
        {{ form_label(form1.IDUser , 'User ID') }}
        {{ form_widget(form1.IDUser, {'attr': {'class': 'form-control' }}) }}

    </div>



    <button type="submit" class="btn btn-primary mt-5"><a href="{{ path('app_affSuiLiv') }}">Register</a></button>
    {{ form_end(form1) }}
</form>
{% endblock %}