{% extends "User/base.html.twig" %}


{% block group %}   {% endblock %}
{% block event %}   {% endblock %}
{% block market %}   {% endblock %}
{% block quiz %}   {% endblock %}
{% block reclamation %}   {% endblock %}

{% block body %}


<!DOCTYPE html>
<html>
<head>
    <!-- Ajoutez ici les balises meta, les liens CSS, etc. -->
</head>
<body>

	<div class="intro-y box p-5 mt-5">

    <form  method="post" >
		{{ form_start(form1 ) }}
        
        <div class="input-form mt-3">
			
			{{ form_label(form1.object , 'OBJECT') }}
			{{ form_widget(form1.object, {'attr': {'class': 'form-control' ,'placeholder' :'OBJECT'}}) }}
			<div style="color:red">
				{{ form_errors(form1.object) }}
			</div>
        </div>

		<div class="input-form">
			{{ form_label(form1.description_Rec , 'DESCRIPTION') }}
			{{ form_widget(form1.description_Rec, {'attr': {'class': 'form-control' ,'placeholder' :'DESCRIPTION'}}) }}
			<div style="color:red">
				{{ form_errors(form1.description_Rec) }} 
			</div>
        </div>

		
        <div class="input-form mt-3">
			{{ form_label(form1.categorie , 'CATEGORY') }}
			{{ form_widget(form1.categorie, {'attr': {'class': 'form-control' ,'placeholder' :'CATEGORY'}}) }}
			<div style="color:red">
				{{ form_errors(form1.categorie) }} 
			</div>
        </div>
        
		<button class="btn btn-raised btn-primary btn-round waves-effect"{{ button_label|default('Save') == 'Save' ? ' formnovalidate' }}>Save</button>		
		{{ form_end(form1) }}
	
    </form>
</div>
</body>
</html>


 {% endblock %}

